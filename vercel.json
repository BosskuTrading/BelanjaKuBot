{
  version 2,
  builds [
    { src api.py, use @vercelpython }
  ],
  routes [
    { src bot1, dest apibot1.py },
    { src bot2, dest apibot2.py }
  ],
  crons [
    {
      path bot2,
      schedule 0 0           Harian jam 8 pagi MYT
    },
    {
      path bot2type=weekly,
      schedule 0 0   1        Mingguan - setiap Isnin
    },
    {
      path bot2type=monthly,
      schedule 0 0 1          Bulanan - setiap 1hb
    }
  ]
}
